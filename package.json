{
  "name": "unplugin-react-routes",
  "version": "0.0.0",
  "description": "File-based routing, similar to Next.js App Router.",
  "keywords": [
    "unplugin",
    "vite"
  ],
  "homepage": "https://github.com/zjy040525/unplugin-react-routes#readme",
  "bugs": {
    "url": "https://github.com/zjy040525/unplugin-react-routes/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zjy040525/unplugin-react-routes.git"
  },
  "license": "MIT",
  "author": "jiayao.zhao",
  "type": "module",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/index.d.cts",
        "import": "./dist/index.d.ts"
      },
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./client.d.ts"
    },
    "./options": {
      "types": {
        "require": "./dist/options.d.cts",
        "import": "./dist/options.d.ts"
      },
      "require": "./dist/options.cjs",
      "import": "./dist/options.js"
    },
    "./types": {
      "types": {
        "require": "./dist/types.d.cts",
        "import": "./dist/types.d.ts"
      },
      "require": "./dist/types.cjs",
      "import": "./dist/types.js"
    },
    "./vite": {
      "types": {
        "require": "./dist/vite.d.cts",
        "import": "./dist/vite.d.ts"
      },
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "./client": [
        "./client.d.ts"
      ],
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist",
    "client.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "play:build": "npm -C playground run build",
    "play:dev": "npm -C playground run dev",
    "play:typed-check": "npm -C playground run typed-check",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typed-check": "tsc -b"
  },
  "dependencies": {
    "fast-glob": "3.3.2",
    "module-from-string": "3.3.1",
    "tsup": "8.3.5",
    "typescript": "5.7.2",
    "unplugin": "2.1.0"
  },
  "devDependencies": {
    "@types/node": "20.17.10",
    "bumpp": "9.9.1",
    "prettier": "3.4.2",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-packagejson": "2.5.6",
    "vitest": "2.1.8"
  },
  "peerDependencies": {
    "react-router": "^7",
    "react-router-dom": "^6",
    "vite": ">=3"
  },
  "peerDependenciesMeta": {
    "react-router": {
      "optional": true
    },
    "react-router-dom": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.14.4",
  "engines": {
    "node": ">=18"
  },
  "volta": {
    "node": "20.18.1",
    "pnpm": "9.14.4"
  }
}
